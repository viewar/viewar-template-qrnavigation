{
  "name": "qr-navigation",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/viewar/viewar-template-qrnavigation.git"
  },
  "scripts": {
    "test": "node ./scripts/run-tests.js './src/**/*.test.js' | faucet",
    "deploy": "viewar-cli deploy",
    "clean": "rimraf build && rimraf bundle",
    "build": "npm run clean && webpack -p --config ./config/webpack.config.js --mode production --env production",
    "start": "npm run clean && webpack-dev-server --config ./config/webpack.config.js --open --mode development --env development_core",
    "start:mock": "npm run clean && webpack-dev-server --config ./config/webpack.config.js --open --mode development --env development_mock",
    "format": "prettier --single-quote --trailing-comma es5 'src/**/*.{js,jsx,css,scss,md,json}' --write"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,jsx}": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "author": "Karl Hofer <kh@viewar.com>",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.2",
    "classnames": "^2.2.5",
    "fastclick": "^1.0.6",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.2.2",
    "react-tap-event-plugin": "^2.0.1",
    "recompose": "^0.26.0",
    "rxjs": "^5.4.2",
    "socket.io-client": "^2.0.4",
    "viewar-api": "^0.36.3",
    "viewar-core": "^11.18.1",
    "webpack": "^4.25.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "faucet": "0.0.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.4",
    "lint-staged": "^8.0.4",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.10.0",
    "postcss": "^8.2.10",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.4.0",
    "prettier": "^1.15.2",
    "react-hot-loader": "^4.3.12",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.4"
  }
}
